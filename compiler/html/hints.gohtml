{{define "Hints"}}
    <h2 id="_Aufgaben" class="font-medium text-lg mb-2">Offene Aufgaben</h2>
    <ul>
        {{range .Hints}}
            <li>
                <div class="flex m-1 ml-4">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                         stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12"/>
                    </svg>
                    <div>{{.}}</div>
                </div>

            </li>
        {{end}}
    </ul>

    {{if .NamedTasks}}
        {{range .NamedTasks}}
            <h2 id="_Aufgaben_{{.Name}}" class="font-medium text-lg mb-2">Aufgaben fÃ¼r {{.Name}}</h2>
            <ul>
                {{range .Tasks}}
                    <li>
                        <div class="flex m-1 ml-4">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                                 stroke="currentColor" class="w-6 h-6">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12"/>
                            </svg>
                            <div>{{.}}</div>
                        </div>

                    </li>
                {{end}}
            </ul>
        {{end}}
    {{end}}
{{end}}